{
  "name": "uuid-by-string",
  "version": "0.6.0",
  "description": "Generating uuid-hash from string",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danakt/uuid-by-string.git"
  },
  "keywords": [
    "uuid",
    "guid"
  ],
  "author": "Danakt Frost",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Danakt/uuid-by-string/issues"
  },
  "homepage": "https://github.com/Danakt/uuid-by-string",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.3",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/*.+(ts|tsx|js)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**",
      "ui/**"
    ]
  }
}
